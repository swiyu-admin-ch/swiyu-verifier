{
  "interactiveLogin": true,
  "httpServer": "NettyWrapper",
  "tokenCallbacks": [
    {
      "issuerId": "verifier_mgmt",
      "tokenExpiry": 36000,
      "requestMappings": [
        {
          "requestParam": "scope",
          "match": "scope1",
          "claims": {
            "sub": "subByScope",
            "aud": [
              "audByScope"
            ]
          }
        }
      ]
    }
  ]
}